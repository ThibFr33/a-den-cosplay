<%= form_with model: @member, local: true, html: { multipart: true } do |form| %>
  <div class="mb-3">
    <%= form.label :photos, "Ajouter des photos" %><br>
    <%= form.file_field :photos, multiple: true, direct_upload: true, class: "form-control" %>
  </div>

  <div class="mt-3">
    <%= form.submit "Enregistrer", class: "btn btn-primary" %>
  </div>
<% end %>

